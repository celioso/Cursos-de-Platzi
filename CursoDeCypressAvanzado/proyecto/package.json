{
  "name": "proyecto",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress open",
    "test-update-snapshot": "cypress open --env updateSnapshots=true",
    "variable:entorno": "cypress open --env VARIABLE_DE_ENTORNO=valor",
    "variable:entorno:sistema": "set NODE_ENV=VARIABLE_DE_DESARROLLO && cypress open",
    "variable:entorno:sistemalinux": "export NODE_ENV=VARIABLE_DE_DESARROLLO && cypress open",
    "cucumber": "cypress run",
    "cucumber:tags": "cypress run --env tags=@probando",
    "delete:results": "rm -r cypress/results/* || true",
    "report": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "mochawesome:report": "npx mochawesome-merge \"cypress/results/mochawesome/*.json\" > mochawesome.json && npx marge mochawesome.json",
    "junit:reports": "jrm cypress/results/junit/combined-report.xml \"cypress/results/junit/*.xml\"",
    "allure:report": "npx cypress run --env allure=true",
    "allure:server": "npx allure serve allure-results",
    "allure:serve:docker": "allure server dockerReports",
    "run:dashboard": "npx cypress run --record --key 56b5e2ce-3e32-4d66-874f-sdfsdrsfdsfsd"
  },
  "keywords": [],
  "author": "Mario Alexander Vargas Celis",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@badeball/cypress-cucumber-preprocessor": "^20.0.7",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.29.0",
    "allure-cypress": "^3.0.0-beta.3",
    "babel-loader": "^9.1.3",
    "cypress": "^13.12.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-xpath": "^2.0.1",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome-merge": "^4.3.0",
    "prettier": "^3.3.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "cy2": "^4.0.9",
    "cypress-cloud": "^1.10.2",
    "cypress-multi-reporters": "^1.6.4",
    "junit-report-merger": "^7.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0"
  }
}
