    "import pandas as pd\n",
    "\n",
    "data = {\n",
    "    'producto': ['A', 'B', 'A', 'B'],\n",
    "    'ventas': [10, 20, 30, 40]\n",
    "}\n",
    "\n",
    "df = pd.DataFrame(data)\n",
    "grupo = df.groupby('producto').agg({'ventas':['sum', 'mean']})\n",
    "print(grupo)"
